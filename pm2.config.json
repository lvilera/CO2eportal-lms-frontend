{
  "apps": [
    {
      "name": "lms-front",
      "script": "bun run start",
      "instances": "max",
      "exec_mode": "cluster",
      "watch": false,
      "max_memory_restart": "1G",
      "env": {
        "NODE_ENV": "development"
      },
      "env_production": {
        "NODE_ENV": "production"
      },
      "error_file": "./logs/error.log",
      "out_file": "./logs/out.log",
      "merge_logs": true,
      "log_date_format": "YYYY-MM-DD HH:mm:ss"
    }
  ]
}
