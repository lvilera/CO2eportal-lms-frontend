{
  "apps": [
    {
      "name": "lms-front",
      "script": "node_modules/next/dist/bin/next",
      "args": "start",
      "cwd": "/var/www/CO2eportal-lms-frontend",
      "instances": 2,
      "exec_mode": "cluster",
      "env": {
        "NODE_ENV": "development",
        "PORT": 3000
      },
      "error_file": "/var/log/pm2/lms-front-error.log",
      "out_file": "/var/log/pm2/lms-front-out.log",
      "merge_logs": true,
      "time": true
    }
  ]
}
